<table class="table">
  <thead>
    <tr>
      <th scope="col">Resolucion</th>
      <th scope="col">Fecha Entrada</th>
      <th scope="col">Fecha Salida</th>
      {% if expediente.agrupador %}
        <th scope="col">Titular Agrupado</th>
      {% endif %}
      {% if expediente.plurianual %}
        <th scope="col">
          Etapa
        </th>
    {% endif %}
      <th scope="col">Observacion</th>
    </tr>
  </thead>
  <tbody>
    {% for item in disposicionesProvinciales %}
      <tr>
        <td>{{item.resolucionNumero}}</td>
        <td>{{item.fechaEntrada|date('d-m-Y')}}</td>
        <td>{{item.fechaSalida|date('d-m-Y')}}</td>
        {% if expediente.agrupador %}
        <td>{% if item.titularAgrupado %}<a href="{{path('titulares_show', { 'id': item.titularAgrupado.id })}}">{{item.titularAgrupado}}</a>{% endif %}
          {% endif %}  </td>

        {% if expediente.plurianual %}
          <td>{{item.etapa}}</td>
        {% endif %}
        <td>{{item.observacion}}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>
