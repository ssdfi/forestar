<form name="profesionales_search" class="form-horizontal" method="get" accept-charset="UTF-8">
	<div id="profesionales">
		<div class="form-group">
			<label class="control-label" for="profesionales_nombre">Apellido y Nombre</label>
			<input id="profesionales_nombre" class="form-control" value="{% if param.nombre is defined %}{{param.nombre}}{% endif %}" name="profesionales_search[nombre]" type="text">
		</div>
		<div class="form-group">
			<label class="control-label" for="profesionales_documento">DNI</label>
			<input id="profesionales_documento" class="form-control" value="{% if param.documento is defined %}{{param.documento}}{% endif %}" name="profesionales_search[documento]" type="text">
		</div>
		<div class="form-group">
			<label class="control-label" for="profesionales_cuit">CUIT</label>
			<input id="profesionales_cuit" class="form-control" value="{% if param.cuit is defined %}{{param.cuit}}{% endif %}" name="profesionales_search[cuit]" type="text">
		</div>
	</div>
	<input type="hidden" name="_csrf_token" value="{{ csrf_token('profesionales') }}">
	<input id="search" class="btn btn-primary pull-right" value="Buscar" type="submit">
</form>
