{% block historialLegalPrototype %}
  <td>
    <div class="col-sm-12">
      {{ form_widget(form.fechaInicio) }}
      {{ form_errors(form.fechaInicio) }}
    </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.fechaFin) }}
        {{ form_errors(form.fechaFin) }}
      </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.observacion) }}
        {{ form_errors(form.observacion) }}
      </div>
  </td>
  <td>
    <a class="btn btn-danger btn-sm pull-right remove-historial-legal remove_nested_fields" data-association="historial_legales">
      <span class="fa fa-remove text-white" aria-hidden="true"></span>
    </a>
  </td>
{% endblock %}
