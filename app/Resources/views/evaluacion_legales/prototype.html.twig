{% block evaluacionLegalPrototype %}
<td>
  <div class="col-sm-12">
    {{ form_widget(form.aprobado) }}
    {{ form_errors(form.aprobado) }}
  </div>
</td>
<td>
  <div class="col-sm-12">
    {{ form_widget(form.resolucion) }}
    {{ form_errors(form.resolucion) }}
  </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.fecha) }}
      {{ form_errors(form.fecha) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.observacion) }}
      {{ form_errors(form.observacion) }}
    </div>
</td>
  <td>
    <a class="btn btn-danger btn-sm pull-right remove-evaluacion-legal remove_nested_fields" data-association="evaluacion_legales">
      <span class="fa fa-remove text-white" aria-hidden="true"></span>
    </a>
  </td>
{% endblock %}
