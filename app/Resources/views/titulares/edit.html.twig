{% extends 'base.html.twig' %}
{% block title %}{{parent()}} - Editar Titular{% endblock %}
{% form_theme edit_form 'bootstrap_4_layout.html.twig' %}
{% block body %}
  <div class="col-md-12">
    <div class="row">
      <div class="col-md-5">
        <h1>Editar Titular</h1>
          {{ form_start(edit_form) }}
              {{ form_widget(edit_form) }}
              <button type="submit" class="btn btn-primary pull-right">Editar</button>
          {{ form_end(edit_form) }}
      </div>
    </div>
  </div>
{% endblock %}
{% block javascripts %}
  {{parent()}}
  <script src="{{ asset('js/titulares_form.js') }}"></script>
{% endblock %}
