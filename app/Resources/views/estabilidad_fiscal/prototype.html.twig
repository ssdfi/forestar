  <td>
    <a class="btn btn-danger btn-sm pull-right remove-estabilidad-fiscal remove_nested_fields" data-association="estabilidad_fiscales">
      <span class="fa fa-remove text-white" aria-hidden="true"></span>
    </a>
  </td>
