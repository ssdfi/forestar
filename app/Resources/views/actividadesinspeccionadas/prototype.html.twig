		<td data-attr='alturaPromedioPlantacion'>
			<div class="col-sm-12">
				{{ form_widget(form.alturaPromedioPlantacion) }}
				{{ form_errors(form.alturaPromedioPlantacion) }}
			</div>
		</td>
