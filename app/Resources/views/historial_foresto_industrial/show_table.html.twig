<div class="table-responsive">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Fecha Realizaci√≥n</th>
        <th scope="col">Estado Foresto Industria</th>
        {% if expediente.agrupador %}
          <th scope="col">Titular Agrupado</th>
        {% endif %}
        {% if expediente.plurianual %}
          <th scope="col">Etapa</th>
        {% endif %}
        <th scope="col">Observaciones</th>
      </tr>
    </thead>
    <tbody>
      {% for item in historialForestoIndustriales %}
        <tr>
          <td>{{item.fechaInicio|date('m-Y')}}</td>
          <td>{% if item.esForestoIndustrial %}<span class="fa fa-check"></span>{% else %}<span class="fa fa-times"></span>{% endif %}</td>
          {% if expediente.agrupador %}
            <td>{% if item.titularAgrupado %}<a href="{{path('titulares_show', { 'id': item.titularAgrupado.id })}}">{{item.titularAgrupado}}</a>{% endif %}</th>
          {% endif %}
          {% if expediente.plurianual %}
            <td>{{item.etapa}}</td>
          {% endif %}
          <td>{{item.observacion}}</td>
        </tr>
      {% endfor %}
    </tbody>
  </table>
</div>
