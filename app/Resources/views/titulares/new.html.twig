{% extends 'base.html.twig' %}
{% block title %}{{parent()}} - Nuevo Titular{% endblock %}
{% form_theme form 'bootstrap_4_layout.html.twig' %}
{% block body %}
<div class="col-md-12">
  <div class="row">
    <div class="col-md-5">
      <h1>Nuevo Titular</h1>
        {{ form_start(form) }}
            {{ form_widget(form) }}
            <button type="submit" class="btn btn-primary pull-right">Crear</button>
        {{ form_end(form) }}
    </div>
  </div>
</div>
{% endblock %}
