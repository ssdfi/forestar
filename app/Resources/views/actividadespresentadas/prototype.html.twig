      {{ form_row(form.especie) }}
      {{ form_errors(form.especie) }}
    </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_row(form.superficieHa) }}
        {{ form_errors(form.superficieHa) }}
      </div>
  </td>
  <td>
    <div class="col-sm-12">
      {{ form_row(form.densidad) }}
      {{ form_errors(form.densidad) }}
