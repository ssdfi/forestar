{% block documentacionPrototype %}
<td>
  <div class="col-sm-12">
    {{ form_widget(form.tipoDocumentacion) }}
    {{ form_errors(form.tipoDocumentacion) }}
  </div>
</td>
<td>
  <div class="col-sm-12">
    {{ form_widget(form.nombreArchivo) }}
    {{ form_errors(form.nombreArchivo) }}
  </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.fechaPresentacion) }}
      {{ form_errors(form.fechaPresentacion) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.observacion) }}
      {{ form_errors(form.observacion) }}
    </div>
</td>
  <td>
    <a class="btn btn-danger btn-sm pull-right remove-documentacion remove_nested_fields" data-association="documentaciones">
      <span class="fa fa-remove text-white" aria-hidden="true"></span>
    </a>
  </td>
{% endblock %}
