{% block controlPlaga %}
<td>
  <div class="col-sm-12">
    {{ form_widget(form.nombre) }}
    {{ form_errors(form.nombre) }}
  </div>
</td>
<td>
  <div class="col-sm-12">
    {{ form_widget(form.accion) }}
    {{ form_errors(form.accion) }}
  </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.principioActivo) }}
      {{ form_errors(form.principioActivo) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.nombrePlaga) }}
      {{ form_errors(form.nombrePlaga) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.periodo) }}
      {{ form_errors(form.periodo) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.dosis) }}
      {{ form_errors(form.dosis) }}
    </div>
</td>
<td>
  <a class="btn btn-danger btn-sm pull-right remove-control-plaga remove_nested_fields" data-association="control_plagas">
    <span class="fa fa-remove text-white" aria-hidden="true"></span>
  </a>
</td>
{% endblock %}
