{% block preparacionSuelo %}
<td>
  <div class="col-sm-12">
    {{ form_widget(form.tipoPreparacionSuelo) }}
    {{ form_errors(form.tipoPreparacionSuelo) }}
  </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.observacion) }}
      {{ form_errors(form.observacion) }}
    </div>
</td>
<td>
  <a class="btn btn-danger btn-sm pull-right remove-preparacion-suelo remove_nested_fields" data-association="preparaciones_SUELO">
    <span class="fa fa-remove text-white" aria-hidden="true"></span>
  </a>
</td>
{% endblock %}
