			<div class="row">
				<div class="col-sm-3">
				</div>
				<div class="col-sm-3">
					<div class="card">
						<canvas id="myChart" width="100" height="100"></canvas>
						<div class="card-body">
							<h5 class="card-title">Expedientes ingresados</h5>
						</div>
					</div>
				</div>
				<div class="col-sm-3">
					<div class="card">
						<canvas id="oilChart" width="100" height="100"></canvas>
						<div class="card-body">
							<h5 class="card-title">Distribución de Expedientes por Área</h5>
						</div>
					</div>
				</div>
				<div class="col-sm-3">
				</div>
			</div>
		</div>
	</div>
{% endblock %}

{% block stylesheets %}
	{{parent()}}
{% endblock %}

{% block javascripts %}
	{{parent()}}
	<script src="{{asset('js/Chart.js')}}"></script>
	<script src="{{asset('js/graphs.js')}}"></script>
{% endblock %}
