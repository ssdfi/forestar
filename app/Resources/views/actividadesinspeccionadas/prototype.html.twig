{% block actividadInspeccionadaPrototype %}
  <td>
    <div class="col-sm-12">
      {{ form_widget(form.especie) }}
      {{ form_errors(form.especie) }}
    </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.superficieHa) }}
        {{ form_errors(form.superficieHa) }}
      </div>
  </td>
  <td>
    <div class="col-sm-12">
      {{ form_widget(form.densidad) }}
      {{ form_errors(form.densidad) }}
    </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.fechaInicio) }}
        {{ form_errors(form.fechaInicio) }}
      </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.fechaFin) }}
        {{ form_errors(form.fechaFin) }}
      </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.departamento) }}
        {{ form_errors(form.departamento) }}
      </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.tipoActividad) }}
        {{ form_errors(form.tipoActividad) }}
      </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.responsableId) }}
        {{ form_errors(form.responsableId) }}
      </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.fechaInspeccion) }}
        {{ form_errors(form.fechaInspeccion) }}
      </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.tipoInspeccion) }}
        {{ form_errors(form.tipoInspeccion) }}
      </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.observacion) }}
        {{ form_errors(form.observacion) }}
      </div>
  </td>
  <td>
    <a class="btn btn-danger btn-sm pull-right remove-actividad-inspeccionada remove_nested_fields" data-association="actividades_inspeccionadas">
      <span class="fa fa-remove text-white" aria-hidden="true"></span>
    </a>
  </td>
{% endblock %}
