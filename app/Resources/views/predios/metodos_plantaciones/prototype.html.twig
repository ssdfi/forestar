{% block metodoPlantacion %}
<td>
  <div class="col-sm-12">
    {{ form_widget(form.tipoMetodoPlantacion) }}
    {{ form_errors(form.tipoMetodoPlantacion) }}
  </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.observacion) }}
      {{ form_errors(form.observacion) }}
    </div>
</td>
<td>
  <a class="btn btn-danger btn-sm pull-right remove-metodo-plantacion remove_nested_fields" data-association="metodos_plantaciones">
    <span class="fa fa-remove text-white" aria-hidden="true"></span>
  </a>
</td>
{% endblock %}
