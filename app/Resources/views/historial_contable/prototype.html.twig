{% block historialContablePrototype %}
<td>
  <div class="col-sm-12">
    {{ form_widget(form.fechaInicio) }}
    {{ form_errors(form.fechaInicio) }}
  </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.fechaFin) }}
      {{ form_errors(form.fechaFin) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.area) }}
      {{ form_errors(form.area) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.estadoContable) }}
      {{ form_errors(form.estadoContable) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.estadoPromocion) }}
      {{ form_errors(form.estadoPromocion) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.estadoSig) }}
      {{ form_errors(form.estadoSig) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.estadoLegal) }}
      {{ form_errors(form.estadoLegal) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.estadoPrincipal) }}
      {{ form_errors(form.estadoPrincipal) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.estadoForestoIndustriales) }}
      {{ form_errors(form.estadoForestoIndustriales) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.observacion) }}
      {{ form_errors(form.observacion) }}
    </div>
</td>
  <td>
    <a class="btn btn-danger btn-sm pull-right remove-historial-contable remove_nested_fields" data-association="historial_contables">
      <span class="fa fa-remove text-white" aria-hidden="true"></span>
    </a>
  </td>
{% endblock %}
