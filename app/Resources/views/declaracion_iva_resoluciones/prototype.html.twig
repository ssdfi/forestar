{% block declaracionIvaPrototype %}
<td>
  <div class="col-sm-12">
    {{ form_widget(form.concepto) }}
    {{ form_errors(form.concepto) }}
  </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.cantidad) }}
      {{ form_errors(form.cantidad) }}
    </div>
</td>
<td>
  <div class="col-sm-12">
    {{ form_widget(form.superficie) }}
    {{ form_errors(form.superficie) }}
  </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.importeTotalGravado) }}
      {{ form_errors(form.importeTotalGravado) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.importeComputableCreditoFiscal) }}
      {{ form_errors(form.importeComputableCreditoFiscal) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.anioFiscal) }}
      {{ form_errors(form.anioFiscal) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.observacion) }}
      {{ form_errors(form.observacion) }}
    </div>
</td>
  <td>
    <a class="btn btn-danger btn-sm pull-right remove-declaracion-iva remove_nested_fields" data-association="declaraciones_iva">
      <span class="fa fa-remove text-white" aria-hidden="true"></span>
    </a>
  </td>
{% endblock %}
