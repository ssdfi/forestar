{% block actividadPresentadaPrototype %}
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.tipoActividad) }}
        {{ form_errors(form.tipoActividad) }}
      </div>
  </td>
  <td>
    <div class="col-sm-12">
      {{ form_widget(form.especie) }}
      {{ form_errors(form.especie) }}
    </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.superficieHa) }}
        {{ form_errors(form.superficieHa) }}
      </div>
  </td>
  <td>
    <div class="col-sm-12">
      {{ form_widget(form.densidad) }}
      {{ form_errors(form.densidad) }}
    </div>
  </td>
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.fechaRealizacion) }}
        {{ form_errors(form.fechaRealizacion) }}
      </div>
  </td>
  {% if expediente.agrupador == true %}
  <td>
    <div class="form-row">
      <div class="col-md-8 mb-3">
        {{form_widget(form.titularAgrupado)}}
      </div>
      <div class="col-md-4 mb-3">
        <div class="form-row">
          <div class="col-md-6">
            <button id="add-titular-agrupado" data-index='{{form.vars.name}}' data-name="appbundle_expedientes_actividadesPresentadas_" class="btn btn-default add-titular-agrupado" type="button" data-toggle="modal" data-target="#titular-agrupado-modal">
              <span class="fa fa-search" aria-hidden="true"></span>
            </button>
          </div>
          <div class="col-md-6">
            <button id="remove-titular-agrupado" data-index='{{form.vars.name}}' data-name="appbundle_expedientes_actividadesPresentadas_" class="btn btn-default remove-titular-agrupado" type="button">
              <span class="fa fa-remove" aria-hidden="true"></span>
            </button>
          </div>
        </div>
      </div>
    </div>
    {{form_widget(form.titular_agrupado_id)}}
  </td>
  {% endif %}
  <td>
      <div class="col-sm-12">
        {{ form_widget(form.observaciones) }}
        {{ form_errors(form.observaciones) }}
      </div>
  </td>
  <td>
    <a class="btn btn-danger btn-sm pull-right remove-actividad-presentada remove_nested_fields" data-association="actividades_presentadas">
      <span class="fa fa-remove text-white" aria-hidden="true"></span>
    </a>
  </td>
{% endblock %}
