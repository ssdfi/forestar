{% block declaracionIvaPrototype %}
<td>
  <div class="col-sm-12">
    {{ form_widget(form.fechaEmision) }}
    {{ form_errors(form.fechaEmision) }}
  </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.inicioPeriodoFiscal) }}
      {{ form_errors(form.inicioPeriodoFiscal) }}
    </div>
</td>
<td>
  <div class="col-sm-12">
    {{ form_widget(form.finPeriodoFiscal) }}
    {{ form_errors(form.finPeriodoFiscal) }}
  </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.valorBeneficio) }}
      {{ form_errors(form.valorBeneficio) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.tipoMoneda) }}
      {{ form_errors(form.tipoMoneda) }}
    </div>
</td>
<td>
    <div class="col-sm-12">
      {{ form_widget(form.observacion) }}
      {{ form_errors(form.observacion) }}
    </div>
</td>
  <td>
    <a class="btn btn-danger btn-sm pull-right remove-declaracion-iva remove_nested_fields" data-association="declaraciones_iva">
      <span class="fa fa-remove text-white" aria-hidden="true"></span>
    </a>
  </td>
{% endblock %}
